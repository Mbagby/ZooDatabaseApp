<% include ../partials/header %>
<h1> Zoos </h1>
<a href= "/zoos/new"> Add a New Zoo </a>
<table class= "table table-striped table-hover">
<thead>
	<tr>
		<th>Name</th>
		<th>Location</th>
	</tr>
</thead>

<tbody>
<% zoos.forEach(function(zoo){ %>
	<tr>
		<td><a href="/zoos/<%= zoo._id%>"><%=zoo.name%></a></td>
		<td><%= zoo.location%></td>
	</tr>
</tbody>
</table>
<%})%>
<% include ../partials/footer %>